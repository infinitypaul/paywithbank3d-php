{
    "name": "parkwayprojects/paywithbank3d-php",
    "description": "A PHP API wrapper for PayWithBank3D.",
    "keywords": [
        "infinitypaul",
        "paywithbank3d-php",
        "parkwayprojects"
    ],
    "homepage": "https://github.com/parkwayprojects/paywithbank3d-php",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paul Edward",
            "email": "infinitypaul@live.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 5.6.0",
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ParkwayProjects\\PayWithBank3D\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ParkwayProjects\\PayWithBank3D\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
